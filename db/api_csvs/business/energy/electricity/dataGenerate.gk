#!/usr/bin/gawk  -f
BEGIN {
      FS=",";
      OFS=",";
      countryData="false";
      usData="false";	
      print "country","kgCO2PerkWh","units","source"
}
# International Data
$3~/Albania/ {countryData="true"}
countryData=="true"&&$19!~/x/&&$3~/./{
	country=$3;
	gsub(/"/,"",country);
	print country,$19/1000,"Dummy","GHGP"
}
$3~/excluding China/ {countryData="false"}