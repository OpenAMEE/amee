#!/usr/bin/gawk  -f
BEGIN {
      FS=",";
      OFS=",";
      usData="false";	
      print "subregion","kgCO2PerkWh","units","source"
}
# US Data
$2~/ASCC/ {usData="true"}
$3~/./&&usData=="true"{
	subregion=$3;
	gsub(/:.*/,"",subregion);
	gsub(/"/,"",subregion);
        # note a mistyping in the GHG worksheet table
	gsub(/MORE/,"MROE",subregion);
	gsub(/MORW/,"MROW",subregion);
	print subregion,$5/1000,"kgCO2/kWh","GHG Protocol"
}