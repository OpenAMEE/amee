try {
  var s = savingType;
}
catch(err){
  savingType="landfill";
}

if(savingType=="recycling"){
  kgPerMonth * kgCO2RecyclingSavedPerKg;
} else if(savingType=="thermal"){
  kgPerMonth * kgCO2ThermalSavedPerKg;
}  else if(savingType=="embodied"){
  kgPerMonth * kgCO2EmbodiedFossilEnergySavedPerKg;
} else {
  kgPerMonth * kgCO2LandfillSavedPerKg;
}